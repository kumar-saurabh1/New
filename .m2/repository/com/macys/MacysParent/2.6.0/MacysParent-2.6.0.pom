<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <artifactId>ParentParent</artifactId>
        <groupId>com.macys</groupId>
        <version>2.6.0</version>
    </parent>
    <groupId>com.macys</groupId>
    <artifactId>MacysParent</artifactId>
    <packaging>pom</packaging>
    <name>${artifactId}</name>
    <licenses>
        <license>
            <name>Macys.com</name>
            <comments>Commercial License</comments>
        </license>
    </licenses>
    <issueManagement>
        <system>JIRA</system>
        <url>http://jira/secure/Dashboard.jspa</url>
    </issueManagement>
    <organization>
        <name>Macys.com</name>
        <url>http://www.macys.com</url>
    </organization>
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <!-- Dependency Version Control Section -->
        <aspectj.version>1.6.1</aspectj.version>
        <castor.version>1.3.1</castor.version>
        <db2.version>2.8.46</db2.version>
        <servlet-spec.version>2.5</servlet-spec.version>
        <slf4j.version>1.5.11</slf4j.version>
        <spring.version>2.5.6.SEC01</spring.version>
        <!-- Environment -->
        <maxmem>1279</maxmem>
        <maxPermSize>256</maxPermSize>
        <targetJdkVersion>1.8</targetJdkVersion>
        <targetJdk>${JAVA_HOME}</targetJdk>
        <strictApacheVersionStandardBasedFailure>true</strictApacheVersionStandardBasedFailure>
        <!-- Needed for maven-version-aggregator macys.properties file -->
        <artifactID>${project.artifactId}</artifactID>
        <artifactVersion>${project.version}</artifactVersion>
        <enforceDependencyConvergence>false</enforceDependencyConvergence>
    </properties>
    <scm>
        <developerConnection>${vcs.type}${vcs.server}${vcs.project}${vcs.group}${branch}${artifactId}</developerConnection>
        <connection>${vcs.type}${vcs.server}${vcs.project}${vcs.group}${branch}${artifactId}</connection>
        <!--url>http://${vcs.server}viewvc/${vcs.project}${vcs.group}${branch}${artifactId}</url-->
    </scm>
    <distributionManagement>
        <repository>
            <id>MacysNexus</id>
            <layout>default</layout>
            <name>Macys Nexus Deploy Repo</name>
            <url>${releaseDistributionLocaltion}</url>
        </repository>
        <snapshotRepository>
            <id>MacysNexus</id>
            <layout>default</layout>
            <uniqueVersion>false</uniqueVersion>
            <name>Macys Nexus Deploy Repo</name>
            <url>${snapshotDistributionLocaltion}</url>
        </snapshotRepository>
        <site>
            <id>ReportsLocation</id>
            <name>Report Locations</name>
            <url>${siteDistributionLocaltion}</url>
        </site>
    </distributionManagement>
    <profiles>
        <profile>
            <id>mac_os_x</id>
            <activation>
                <os>
                    <family>unix</family>
                    <name>mac os x</name>
                </os>
            </activation>
            <properties>
                <JAVA15_HOME>/System/Library/Frameworks/JavaVM.framework/Versions/1.5.0/Home</JAVA15_HOME>
            </properties>
        </profile>
            <profile>
                <id>reports</id>
                <activation>
                    <activeByDefault>false</activeByDefault>
                </activation>
                <build>
                    <plugins>
                        <plugin>
                            <groupId>org.codehaus.mojo</groupId>
                            <artifactId>cobertura-maven-plugin</artifactId>
                            <version>2.5.1</version>
                            <configuration>
                                <formats>
                                    <format>xml</format>
                                </formats>
                                <check>
                                    <branchRate>0</branchRate>
                                    <lineRate>0</lineRate>
                                    <haltOnFailure>false</haltOnFailure>
                                    <totalBranchRate>0</totalBranchRate>
                                    <totalLineRate>0</totalLineRate>
                                    <packageLineRate>0</packageLineRate>
                                    <packageBranchRate>0</packageBranchRate>
                                    <regexes>
                                        <regex>
                                            <pattern>com.example.reallyimportant.*</pattern>
                                            <branchRate>0</branchRate>
                                            <lineRate>0</lineRate>
                                        </regex>
                                        <regex>
                                            <pattern>com.example.boringcode.*</pattern>
                                            <branchRate>0</branchRate>
                                            <lineRate>0</lineRate>
                                        </regex>
                                    </regexes>
                                </check>
                            </configuration>
                            <executions>
                                <execution>
                                    <phase>post-integration-test</phase>
                                    <goals>
                                        <goal>clean</goal>
                                        <goal>cobertura</goal>
                                    </goals>
                                </execution>
                            </executions>
                        </plugin>
                    </plugins>
                </build>
            </profile>
    </profiles>
    <build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>properties-maven-plugin</artifactId>
            </plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>buildnumber-maven-plugin</artifactId>
            </plugin>
            <plugin>
                <artifactId>maven-source-plugin</artifactId>
                <inherited>true</inherited>
            </plugin>
            <!-- Disabling a few build cleanup dependencies until future when time allows
                <plugin>
                <artifactId>maven-dependency-plugin</artifactId>
                <inherited>true</inherited>
                </plugin>
                <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>clirr-maven-plugin</artifactId>
                </plugin>
            -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-enforcer-plugin</artifactId>
            </plugin>
        </plugins>
        <pluginManagement>
            <plugins>
                <!-- Disabling a few build cleanup dependencies until future when time allows
                    <plugin>
                    <groupId>org.codehaus.mojo</groupId>
                    <artifactId>clirr-maven-plugin</artifactId>
                    <version>2.3.0</version>
                    <configuration>
                    <apacheVersionStandardBasedFailure>true</apacheVersionStandardBasedFailure>
                    <strictApacheVersionStandardBasedFailure>${strictApacheVersionStandardBasedFailure}</strictApacheVersionStandardBasedFailure>
                    <logResults>true</logResults>
                    </configuration>
                    <executions>
                    <execution>
                    <goals>
                    <goal>check</goal>
                    </goals>
                    <phase>process-classes</phase>
                    </execution>
                    </executions>
                    </plugin>
                -->
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-enforcer-plugin</artifactId>
                    <version>1.0</version>
                    <dependencies>
                        <dependency>
                            <groupId>org.apache.maven.enforcer.rules</groupId>
                            <artifactId>maven-enforce-aggregatorversion</artifactId>
                            <version>1.0.0</version>
                        </dependency>
                    </dependencies>
                    <executions>
                        <execution>
                            <id>enforce</id>
                            <configuration>
                                <rules>
                                    <myCustomRule implementation="org.apache.maven.enforcer.rule.ReactorVersion" />
                                    <requireReleaseDeps>
                                        <onlyWhenRelease>true</onlyWhenRelease>
                                        <message>No Snapshots Allowed!</message>
                                    </requireReleaseDeps>
                                </rules>
                            </configuration>
                            <goals>
                                <goal>enforce</goal>
                            </goals>
                        </execution>
                    </executions>
                </plugin>
                <plugin>
                    <groupId>org.codehaus.mojo</groupId>
                    <artifactId>buildnumber-maven-plugin</artifactId>
                    <executions>
                        <execution>
                            <phase>validate</phase>
                            <goals>
                                <goal>create</goal>
                            </goals>
                        </execution>
                    </executions>
                    <configuration>
                        <doCheck>false</doCheck>
                        <doUpdate>false</doUpdate>
                        <revisionOnScmFailure>nosvnversion</revisionOnScmFailure>
                        <timestampFormat>{0, date, EEE MMM d HH:mm:ss z yyyy}</timestampFormat>
                        <providerImplementations>
                            <svn>javasvn</svn>
                        </providerImplementations>
                    </configuration>
                </plugin>
                <!-- Disabling a few build cleanup dependencies until future when time allows
                    <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-dependency-plugin</artifactId>
                    <version>2.1</version>
                    <executions>
                    <execution>
                    <phase>package</phase>
                    <goals>
                    <goal>analyze-only</goal>
                    </goals>
                    </execution>
                    </executions>
                    <configuration>
                    <failOnWarning>true</failOnWarning>
                    <ignoreNonCompile>true</ignoreNonCompile>
                    </configuration>
                    </plugin>
                -->
                <plugin>
                    <groupId>org.codehaus.mojo</groupId>
                    <artifactId>properties-maven-plugin</artifactId>
                    <version>1.0-alpha-2</version>
                    <executions>
                        <execution>
                            <phase>generate-resources</phase>
                            <goals>
                                <goal>write-project-properties</goal>
                            </goals>
                            <configuration>
                                <outputFile> ${project.build.outputDirectory}/META-INF/macys.properties </outputFile>
                            </configuration>
                        </execution>
                    </executions>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-compiler-plugin</artifactId>
                    <configuration>
                        <target>${targetJdkVersion}</target>
                        <source>${targetJdkVersion}</source>
                        <fork>true</fork>
                        <executable>${targetJdk}/bin/javac</executable>
                        <maxmem>${maxmem}m</maxmem>
                    </configuration>
                </plugin>
                <plugin>
                    <artifactId>maven-assembly-plugin</artifactId>
                    <version>2.2.1</version>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-surefire-plugin</artifactId>
                    <version>2.12</version>
					<inherited>true</inherited>
                    <configuration>
                        <forkMode>once</forkMode>
                        <jvm>${targetJdk}/bin/java</jvm>
                        <argLine>-Xmx${maxmem}m -XX:MaxPermSize=${maxPermSize}m</argLine>
                        <enableAssertions>true</enableAssertions>
                        <parallel>false</parallel>
                        <includes>
                            <include>**/*TestCase.java</include>
                            <include>**/Test*.java</include>
                            <include>**/*Test.java</include>
                            <include>**/*IT.java</include>
                            <include>**/IT*.java</include>
                            <include>**/*ITCase.java</include>
                        </includes>
                        <systemPropertyVariables>
                            <run_known_breaks>false</run_known_breaks>
                            <unit_test_groups>unit,functional</unit_test_groups>
                            <integration_test_groups>remote_integration</integration_test_groups>
                            <basedir>${basedir}</basedir>
                        </systemPropertyVariables>
                        <properties>
                            <property>
                                <name>basedir</name>
                                <value>${basedir}</value>
                            </property>
                        </properties>
                    </configuration>
                </plugin>
                <plugin>
                    <artifactId>maven-failsafe-plugin</artifactId>
                    <version>2.5</version>
                    <configuration>
                        <parallel>false</parallel>
                        <systemPropertyVariables>
                            <forkMode>once</forkMode>
                            <jvm>${targetJdk}/bin/java</jvm>
                            <argLine>-Xmx${maxmem}m -XX:MaxPermSize=${maxPermSize}m</argLine>
                            <run_known_breaks>true</run_known_breaks>
                            <integration_phase>true</integration_phase>
                            <unit_test_groups>unit,functional</unit_test_groups>
                            <integration_test_groups>remote_integration</integration_test_groups>
                            <basedir>${basedir}</basedir>
                            <includes>
                                <include>**/*TestCase.java</include>
                                <include>**/Test*.java</include>
                                <include>**/*Test.java</include>
                                <include>**/*IT.java</include>
                                <include>**/IT*.java</include>
                                <include>**/*ITCase.java</include>
                            </includes>
                        </systemPropertyVariables>
                        <properties>
                            <property>
                                <name>basedir</name>
                                <value>${basedir}</value>
                            </property>
                        </properties>
                    </configuration>
                    <executions>
                        <execution>
                            <id>integration-test</id>
                            <goals>
                                <goal>integration-test</goal>
                            </goals>
                        </execution>
                        <execution>
                            <id>verify</id>
                            <goals>
                                <goal>verify</goal>
                            </goals>
                        </execution>
                    </executions>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-jar-plugin</artifactId>
                    <configuration>
                        <archive>
                            <manifestEntries>
                                <SCM-Revision>${buildNumber}</SCM-Revision>
                            </manifestEntries>
                        </archive>
                    </configuration>
                    <executions>
                        <execution>
                            <id>regular-package</id>
                            <phase>package</phase>
                        </execution>
                    </executions>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-source-plugin</artifactId>
                    <version>2.1.1</version>
                    <inherited>true</inherited>
                    <executions>
                        <execution>
                            <id>attach-sources</id>
                            <phase>package</phase>
                            <goals>
                                <goal>jar-no-fork</goal>
                            </goals>
                        </execution>
                    </executions>
                </plugin>
                <plugin>
                    <artifactId>maven-deploy-plugin</artifactId>
                    <version>2.5</version>
                </plugin>
                <!--plugin>
                    <groupId>org.codehaus.mojo</groupId>
                    <artifactId>cobertura-maven-plugin</artifactId>
                    <version>2.5.1</version>
                    <configuration>
                        <formats>
                            <format>xml</format>
                        </formats>
                        <check>
                            <branchRate>0</branchRate>
                            <lineRate>0</lineRate>
                            <haltOnFailure>false</haltOnFailure>
                            <totalBranchRate>0</totalBranchRate>
                            <totalLineRate>0</totalLineRate>
                            <packageLineRate>0</packageLineRate>
                            <packageBranchRate>0</packageBranchRate>
                            <regexes>
                                <regex>
                                    <pattern>com.example.reallyimportant.*</pattern>
                                    <branchRate>0</branchRate>
                                    <lineRate>0</lineRate>
                                </regex>
                                <regex>
                                    <pattern>com.example.boringcode.*</pattern>
                                    <branchRate>0</branchRate>
                                    <lineRate>0</lineRate>
                                </regex>
                            </regexes>
                        </check>
                    </configuration>
                    <executions>
                        <execution>
                            <phase>post-integration-test</phase>
                            <goals>
                                <goal>clean</goal>
                                <goal>cobertura</goal>
                            </goals>
                        </execution>
                    </executions>
                </plugin-->
                <plugin>
                    <groupId>org.apache.felix</groupId>
                    <artifactId>maven-bundle-plugin</artifactId>
                    <extensions>true</extensions>
                    <configuration>
                        <instructions>
                            <Release>${pom.version}</Release>
                            <Vendor>${pom.organization.url}</Vendor>
                            <Organization>${pom.organization.name}</Organization>
                            <Bundle-SymbolicName>${project.artifactId}</Bundle-SymbolicName>
                            <Build-Version>${pom.version}</Build-Version>
                            <Bundle-Version>${bundle.branchVersion}</Bundle-Version>
                            <!--
                                Kind of magic to include full versions. For details see
                                http://www.aqute.biz/Code/Bnd#cmd and
                                http://www.mail-archive.com/users@felix.apache.org/msg03129.html
                            -->
                            <_versionpolicy>$(@)</_versionpolicy>
                            <Import-Library>org.springframework.spring;version="${spring.instrumented.version}"</Import-Library>
                        </instructions>
                    </configuration>
                </plugin>
                <plugin>
                    <groupId>com.macys.buildtools.maven.plugin</groupId>
                    <artifactId>maven-phpgeneration-plugin</artifactId>
                    <executions>
                        <execution>
                            <phase>package</phase>
                            <goals>
                                <goal>generateResources</goal>
                            </goals>
                        </execution>
                    </executions>
                </plugin>
                <plugin>
                    <artifactId>maven-jar-plugin</artifactId>
                    <configuration>
                        <archive>
                            <manifestEntries>
                                <SVN-Revision>${buildNumber}</SVN-Revision>
                            </manifestEntries>
                        </archive>
                    </configuration>
                </plugin>
                <plugin>
                    <artifactId>maven-war-plugin</artifactId>
                    <configuration>
                        <archive>
                            <manifestEntries>
                                <SVN-Revision>${buildNumber}</SVN-Revision>
                            </manifestEntries>
                        </archive>
                    </configuration>
                </plugin>
                <plugin>
                    <artifactId>maven-ear-plugin</artifactId>
                    <configuration>
                        <archive>
                            <manifestEntries>
                                <SVN-Revision>${buildNumber}</SVN-Revision>
                            </manifestEntries>
                        </archive>
                    </configuration>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>
