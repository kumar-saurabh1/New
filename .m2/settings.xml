<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <!-- localRepository/>
  <interactiveMode/>
  <usePluginRegistry/>
  <offline/-->
	
  <pluginGroups>
    <pluginGroup>org.apache.maven.plugins</pluginGroup>
    <pluginGroup>org.mortbay.jetty</pluginGroup>
    <pluginGroup>org.codehaus.mojo</pluginGroup>
    <pluginGroup>com.macys.buildtools.maven.plugin.javascript-css-combo-min-maven-plugin</pluginGroup>
    <pluginGroup>com.macys.buildtools.maven.plugin</pluginGroup>
  </pluginGroups>
	
  <servers>
    <server>
       <id>ReportsLocation</id>    
       <directoryPermissions>775</directoryPermissions>
       <filePermissions>775</filePermissions>
       <username>fsgapps</username>
       <password>######</password>
    </server>
    <server>
       <id>MacysNexus</id>
       <directoryPermissions>775</directoryPermissions>
       <filePermissions>775</filePermissions>
       <username></username>
       <password></password>
    </server>  
  </servers>
  <profiles>
    <profile>
      <id>macysSnapshots</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <repositories>
        <repository>
          <id>macysSnapshots</id>
          <name>Macys Snapshots</name>
          <snapshots>
            <enabled>true</enabled>
            <updatePolicy>always</updatePolicy>
            <checksumPolicy>ignore</checksumPolicy>
          </snapshots>
          <releases>
            <enabled>true</enabled>
            <updatePolicy>always</updatePolicy>
            <checksumPolicy>ignore</checksumPolicy>
          </releases>
          <layout>default</layout>
          <url>http://artifacts.devops.fds.com/public-snapshots</url>
        </repository>
      </repositories>
      <properties>
        <releaseDistributionLocaltion>scp://ci/home/macywas/sonatype-work/nexus/storage/releases</releaseDistributionLocaltion>
        <snapshotDistributionLocaltion>scp://ci/home/macywas/sonatype-work/nexus/storage/snapshots</snapshotDistributionLocaltion>
        <siteDistributionLocaltion>scp://11.16.155.31/www/sites/${artifactId}/${version}/</siteDistributionLocaltion>
      </properties>
      <pluginRepositories>
        <pluginRepository>
          <id>MacysInternalSnapshots</id>
          <name>Macys plugin Snapshots</name>
          <snapshots>
            <enabled>true</enabled>
            <updatePolicy>always</updatePolicy>
            <checksumPolicy>ignore</checksumPolicy>
          </snapshots>
          <releases>
            <enabled>true</enabled>
            <updatePolicy>always</updatePolicy>
            <checksumPolicy>ignore</checksumPolicy>
          </releases>
          <layout>default</layout>
          <url>http://artifacts.devops.fds.com/plugin-snapshot</url>
        </pluginRepository>
				<pluginRepository>
          <id>PluginSnapshots</id>
          <name>OSS Snapshots</name>
          <snapshots>
            <enabled>true</enabled>
            <updatePolicy>always</updatePolicy>
            <checksumPolicy>ignore</checksumPolicy>
          </snapshots>
          <releases>
            <enabled>true</enabled>
            <updatePolicy>always</updatePolicy>
            <checksumPolicy>ignore</checksumPolicy>
          </releases>
          <layout>default</layout>
          <url>http://artifacts.devops.fds.com/public-snapshots</url>
        </pluginRepository>        
      </pluginRepositories>	
    </profile>
  </profiles>
  <mirrors>
   <mirror>
      <id>MacysNexus</id>
      <name>Macys.com Nexus (San Francisco)</name>
      <url>http://artifacts.devops.fds.com/public</url>
      <mirrorOf>*,!macysSnapshots,!MacysInternalSnapshots</mirrorOf>
    </mirror>
  </mirrors>
</settings>
